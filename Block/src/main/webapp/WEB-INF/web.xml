<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>register.registerClass</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/toRegister</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>register.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/toLogin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>upload</servlet-name>
    <servlet-class>publish.UploadFile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>upload</servlet-name>
    <url-pattern>/uploadFile</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>showShop</servlet-name>
    <servlet-class>showShop.showShop</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>showShop</servlet-name>
    <url-pattern>/showShop</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addCar</servlet-name>
    <servlet-class>addCar.Car</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addCar</servlet-name>
    <url-pattern>/addCar</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>showCar</servlet-name>
    <servlet-class>Car.showCar</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>showCar</servlet-name>
    <url-pattern>/car</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addConnect</servlet-name>
    <servlet-class>Connected.AddConnect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addConnect</servlet-name>
    <url-pattern>/connect</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>accounts</servlet-name>
    <servlet-class>account.accounts</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>accounts</servlet-name>
    <url-pattern>/pay</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>order</servlet-name>
    <servlet-class>order.getOrder</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>order</servlet-name>
    <url-pattern>/order</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>broadcast</servlet-name>
    <servlet-class>Connected.broadcast</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>broadcast</servlet-name>
    <url-pattern>/broadcast</url-pattern>
  </servlet-mapping>
